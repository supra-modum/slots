{
  "name": "slot-machine",
  "version": "1.0.0",
  "description": "5x3 Slot Machine Game built with Pixi.js and TypeScript",
  "scripts": {
    "start": "parcel serve src/index.html --open --no-cache --no-source-maps",
    "build": "npm run format && npm run clean && parcel build src/index.html",
    "clean": "rimraf dist",
    "format": "prettier --write src",
    "check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
