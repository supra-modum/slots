  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supra-modum/mascot-test/issues"
  },
  "homepage": "https://github.com/supra-modum/mascot-test#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.0",
    "parcel": "^2.8.0",
    "prettier": "^2.8.0",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "pixi.js": "^7.0.4"
  }
